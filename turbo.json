{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"web#build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**"]
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	},
	"globalDependencies": ["tsconfig.json", ".env.*"]
}
